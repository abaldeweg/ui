{
  "$schema": "./../component.schema.json",
  "name": "BSelect",
  "description": "Additional attributes are passed to the underlying input or select element.",
  "props": [
    {
      "name": "modelValue",
      "type": [
        "Array"
      ]
    },
    {
      "name": "type",
      "type": [
        "String"
      ],
      "default": "checkbox",
      "allowedValues": [
        "checkbox",
        "radio",
        "options"
      ]
    },
    {
      "name": "name",
      "type": [
        "String"
      ]
    },
    {
      "name": "id",
      "type": [
        "String"
      ]
    },
    {
      "name": "help",
      "type": [
        "String"
      ]
    },
    {
      "name": "options",
      "type": [
        "Object"
      ]
    }
  ],
  "events": [
    {
      "name": "update:modelValue"
    }
  ]
}
