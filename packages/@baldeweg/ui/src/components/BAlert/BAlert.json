{
  "$schema": "./../component.schema.json",
  "name": "BAlert",
  "props": [
    {
      "name": "type",
      "type": [
        "String"
      ],
      "default": "neutral",
      "allowedValues": [
        "neutral",
        "error",
        "warning",
        "success",
        "info"
      ]
    },
    {
      "name": "closable",
      "type": [
        "Boolean"
      ],
      "default": false
    },
    {
      "name": "elevated",
      "type": [
        "Boolean"
      ],
      "default": false
    }
  ],
  "slots": [
    {
      "name": "default"
    }
  ]
}
