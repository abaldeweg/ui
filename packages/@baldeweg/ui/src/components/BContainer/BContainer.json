{
  "$schema": "./../component.schema.json",
  "name": "BContainer",
  "props": [
    {
      "name": "size",
      "type": [
        "String"
      ],
      "default": "l",
      "allowedValues": [
        "l",
        "m",
        "s"
      ]
    },
    {
      "name": "align",
      "type": [
        "String"
      ],
      "default": "left",
      "allowedValues": [
        "left",
        "right",
        "center"
      ]
    },
    {
      "name": "highlight",
      "type": [
        "Boolean"
      ],
      "default": false
    }
  ],
  "slots": [
    {
      "name": "default"
    }
  ]
}
