{
  "$schema": "./../component.schema.json",
  "name": "BToast",
  "props": [
    {
      "name": "type",
      "type": [
        "String"
      ],
      "allowedValues": [
        "neutral",
        "error",
        "warning",
        "success"
      ],
      "default": "neutral"
    },
    {
      "name": "visible",
      "type": [
        "Boolean"
      ],
      "default": false
    }
  ],
  "slots": [
    {
      "name": "default"
    }
  ]
}
