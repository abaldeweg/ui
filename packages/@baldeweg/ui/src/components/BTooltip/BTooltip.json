{
  "$schema": "./../component.schema.json",
  "name": "BTooltip",
  "props": [
    {
      "name": "position",
      "type": [
        "String"
      ],
      "allowedValues": [
        "top",
        "bottom",
        "left",
        "right"
      ],
      "default": "top"
    },
    {
      "name": "text",
      "type": [
        "String"
      ],
      "example": "Message"
    }
  ],
  "slots": [
    {
      "name": "default"
    }
  ]
}
