{
  "$schema": "./../component.schema.json",
  "name": "BList",
  "props": [
    {
      "name": "route",
      "type": [
        "Object"
      ]
    },
    {
      "name": "start-size",
      "type": [
        "String"
      ],
      "default": "100px"
    },
    {
      "name": "end-size",
      "type": [
        "String"
      ],
      "default": "50px"
    },
    {
      "name": "divider",
      "type": [
        "Boolean"
      ],
      "default": false,
      "example": true
    },
    {
      "name": "hover",
      "type": [
        "Boolean"
      ],
      "default": false,
      "example": true
    }
  ],
  "slots": [
    {
      "name": "default"
    },
    {
      "name": "start"
    },
    {
      "name": "end"
    },
    {
      "name": "title"
    },
    {
      "name": "subtitle"
    }
  ]
}
