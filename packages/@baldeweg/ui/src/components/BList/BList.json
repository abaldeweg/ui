{
  "$schema": "./../component.schema.json",
  "name": "BList",
  "props": [
    {
      "name": "route",
      "type": [
        "Object"
      ]
    },
    {
      "name": "media-size",
      "type": [
        "String"
      ],
      "default": "landscape",
      "allowedValues": [
        "landscape",
        "portrait",
        "avatar"
      ]
    },
    {
      "name": "text-width",
      "type": [
        "String"
      ],
      "default": "200px"
    },
    {
      "name": "controls-width",
      "type": [
        "String"
      ],
      "default": "50px"
    },
    {
      "name": "divider",
      "type": [
        "Boolean"
      ],
      "default": false,
      "example": true
    },
    {
      "name": "hover",
      "type": [
        "Boolean"
      ],
      "default": false,
      "example": true
    }
  ],
  "slots": [
    {
      "name": "default"
    },
    {
      "name": "media"
    },
    {
      "name": "text"
    },
    {
      "name": "controls"
    },
    {
      "name": "title"
    },
    {
      "name": "subtitle"
    }
  ]
}
