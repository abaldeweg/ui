{
  "$schema": "./../component.schema.json",
  "name": "BFormSelect",
  "props": [
    {
      "name": "items",
      "type": [
        "Array"
      ]
    },
    {
      "name": "itemKey",
      "type": [
        "String"
      ],
      "default": "key"
    },
    {
      "name": "itemValue",
      "type": [
        "String"
      ],
      "default": "value"
    },
    {
      "name": "modelValue",
      "type": [
        "Number",
        "String"
      ]
    },
    {
      "name": "focus",
      "type": [
        "Boolean"
      ]
    },
    {
      "name": "allowEmpty",
      "type": [
        "Boolean"
      ],
      "default": false
    }
  ],
  "events": [
    {
      "name": "update:modelValue"
    }
  ]
}
