{
  "name": "BFormSelect",
  "props": [
    {
      "name": "items",
      "type": {
        "name": "array"
      }
    },
    {
      "name": "itemKey",
      "type": {
        "name": "string"
      },
      "defaultValue": {
        "func": false,
        "value": "'key'"
      }
    },
    {
      "name": "itemValue",
      "type": {
        "name": "string"
      },
      "defaultValue": {
        "func": false,
        "value": "'value'"
      }
    },
    {
      "name": "modelValue",
      "type": {
        "name": "number|string"
      }
    },
    {
      "name": "focus",
      "type": {
        "name": "boolean"
      }
    },
    {
      "name": "allowEmpty",
      "type": {
        "name": "boolean"
      },
      "defaultValue": {
        "func": false,
        "value": "false"
      }
    }
  ],
  "events": [
    {
      "name": "update:modelValue"
    }
  ]
}
