{
  "$schema": "./../component.schema.json",
  "name": "BFormInput",
  "description": "Not applicable to checkbox and radio.",
  "props": [
    {
      "name": "modelValue",
      "type": [
        "String",
        "Number"
      ]
    },
    {
      "name": "noStyling",
      "type": [
        "Boolean"
      ],
      "default": false
    },
    {
      "name": "event",
      "type": [
        "Boolean"
      ],
      "default": false
    },
    {
      "name": "focus",
      "type": [
        "Boolean"
      ],
      "default": false
    }
  ],
  "events": [
    {
      "name": "update:modelValue"
    }
  ]
}
