{
  "$schema": "./../component.schema.json",
  "name": "BPanel",
  "props": [
    {
      "name": "modelValue",
      "type": [
        "Boolean"
      ],
      "default": false
    },
    {
      "name": "position",
      "type": [
        "String"
      ],
      "default": "left",
      "allowedValues": [
        "left",
        "right"
      ]
    },
    {
      "name": "width",
      "type": [
        "String"
      ],
      "default": "300px"
    }
  ],
  "slots": [
    {
      "name": "header"
    },
    {
      "name": "default"
    },
    {
      "name": "footer"
    }
  ],
  "events": [
    {
      "name": "update:modelValue"
    }
  ]
}
