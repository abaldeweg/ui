{
  "$schema": "./../component.schema.json",
  "name": "BMaterialIcon",
  "description": "color accepts a string (e.g. hex code, custom properties)",
  "props": [
    {
      "name": "size",
      "type": [
        "Number"
      ],
      "default": "24"
    },
    {
      "name": "color",
      "type": [
        "String"
      ],
      "default": "inherit"
    },
    {
      "name": "hover",
      "type": [
        "Boolean"
      ],
      "default": false
    }
  ],
  "slots": [
    {
      "name": "default"
    }
  ]
}
