{
  "$schema": "./../component.schema.json",
  "name": "BButton",
  "props": [
    {
      "name": "design",
      "type": [
        "String"
      ],
      "default": "primary",
      "allowedValues": [
        "primary",
        "primary_danger",
        "primary_wide",
        "outline",
        "outline_danger",
        "outline_wide",
        "text",
        "text_danger"
      ]
    },
    {
      "name": "ripple",
      "type": [
        "Boolean"
      ],
      "default": false
    }
  ],
  "slots": [
    {
      "name": "default"
    }
  ],
  "events": [
    {
      "name": "click"
    }
  ]
}
