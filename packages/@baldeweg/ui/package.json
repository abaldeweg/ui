{
  "name": "@baldeweg/ui",
  "version": "0.7.0-alpha.2",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test:unit": "vitest --environment jsdom",
    "test:unit:ci": "vitest run --environment jsdom",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "docu": "docu ./docu/json/components.json ./docu/templates/component.ejs ./docu/components.md"
  },
  "dependencies": {
    "vue": "3.2.41"
  },
  "devDependencies": {
    "@baldeweg/docu": "./packages/@baldeweg/docu",
    "@baldeweg/eslint-config": "^0.7.0-alpha.2",
    "@rushstack/eslint-patch": "1.2.0",
    "@vitejs/plugin-vue": "3.2.0",
    "@vue/eslint-config-prettier": "7.0.0",
    "@vueuse/core": "9.4.0",
    "axios": "1.1.3",
    "eslint": "8.27.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-vue": "9.7.0",
    "jsdom": "20.0.2",
    "lodash": "4.17.21",
    "mkdirp": "1.0.4",
    "msw": "0.47.4",
    "prettier": "2.7.1",
    "start-server-and-test": "1.14.0",
    "vite": "3.2.2",
    "vite-plugin-webfont-dl": "3.4.1",
    "vitest": "0.24.5",
    "vue-router": "4.1.6"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/ui.umd.js",
  "module": "./dist/ui.es.js",
  "exports": {
    ".": {
      "import": "./dist/ui.es.js",
      "require": "./dist/ui.umd.js"
    },
    "./styles": "./dist/style.css",
    "./composables/*": "./src/composables/*.js"
  },
  "author": "Andr√© Baldeweg <a@baldeweg.org>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/abaldeweg/ui.git"
  }
}
