{
  "name": "@baldeweg/ui",
  "version": "0.7.2",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test:unit": "vitest --environment jsdom",
    "test:unit:ci": "vitest run --environment jsdom",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "docu": "docu ./docu/json/components.json ./docu/templates/component.ejs ./docu/components.md"
  },
  "dependencies": {
    "vue": "3.2.47"
  },
  "devDependencies": {
    "@baldeweg/docu": "^0.7.1",
    "@baldeweg/eslint-config": "^0.7.1",
    "@rushstack/eslint-patch": "1.2.0",
    "@vitejs/plugin-vue": "4.0.0",
    "@vue/eslint-config-prettier": "7.1.0",
    "@vueuse/core": "9.12.0",
    "axios": "1.3.3",
    "eslint": "8.34.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-vue": "9.9.0",
    "jsdom": "21.1.0",
    "lodash": "4.17.21",
    "mkdirp": "2.1.3",
    "prettier": "2.8.4",
    "start-server-and-test": "1.15.4",
    "vite": "4.1.4",
    "vite-plugin-webfont-dl": "3.6.0",
    "vitest": "0.28.5",
    "vue-router": "4.1.6"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/ui.umd.js",
  "module": "./dist/ui.es.js",
  "exports": {
    ".": {
      "import": "./dist/ui.es.js",
      "require": "./dist/ui.umd.js"
    },
    "./styles": "./dist/style.css",
    "./composables/*": "./src/composables/*.js"
  },
  "author": "Andr√© Baldeweg <a@baldeweg.org>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/abaldeweg/ui.git"
  }
}
