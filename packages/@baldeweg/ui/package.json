{
  "name": "@baldeweg/ui",
  "version": "0.9.8",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test:unit": "vitest --environment jsdom",
    "test:unit:ci": "vitest run --environment jsdom",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "docu": "docu ./docu/json/components.json ./docu/templates/component.ejs ./docu/components.md"
  },
  "dependencies": {
    "vue": "3.4.22"
  },
  "devDependencies": {
    "@baldeweg/docu": "^0.9.8",
    "@baldeweg/eslint-config": "^0.9.8",
    "@rushstack/eslint-patch": "1.10.2",
    "@vitejs/plugin-vue": "5.0.4",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vueuse/core": "10.9.0",
    "axios": "1.6.8",
    "eslint": "9.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-vue": "9.25.0",
    "jsdom": "24.0.0",
    "lodash": "4.17.21",
    "mkdirp": "3.0.1",
    "prettier": "3.2.5",
    "start-server-and-test": "2.0.3",
    "vite": "5.2.9",
    "vite-plugin-webfont-dl": "3.9.3",
    "vitest": "1.5.0",
    "vue-router": "4.3.0"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/ui.umd.js",
  "module": "./dist/ui.es.js",
  "exports": {
    ".": {
      "import": "./dist/ui.es.js",
      "require": "./dist/ui.umd.js"
    },
    "./styles": "./dist/style.css",
    "./composables/*": "./src/composables/*.js"
  },
  "author": "Andr√© Baldeweg <a@baldeweg.org>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/abaldeweg/ui.git"
  },
  "type": "module"
}
